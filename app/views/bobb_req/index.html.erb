<h1>登録ユーザ一覧</h1>

<table class="users">
  <tr>
    <td>User ID</td>
    <td>User Name</td>
    <td>date</td>
    <td>dare2</td>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <td><%= user.id %></td>
    <td><%= user.user_name %></td>
    <td><%= user.updated_at %></td>
    <td><%= user.created_at %></td>
  </tr>
  <% end %>
</table>


<h1>アクセスログ</h1>

<table class="accessList">
  <tr>
    <td>User ID</td>
    <td>User Name</td>
    <td>Level</td>
    <td>アクセス時間</td>
    <td>registration_id</td>
  </tr>
  <% @accessList.each do |access| %>
  <tr>
    <td><%= access.user_id %></td>
    <td><%= access.user_name %></td>
    <td><%= access.user_level  %></td>
    <td><%= access.access_time  %></td>
    <td><%= access.transaction_id %></td>
  </tr>
  <% end %>
</table>

<h1>対戦履歴</h1>

<table class="BattleRecords">
  <tr>
    <td>対戦ID</td>
    <td>対戦要求者ID</td>
    <td>対戦応答者ID</td>
    <td>ステータス</td>
  </tr>
  <% @BattleRecords.each do |record| %>
  <tr>
    <td><%= record.id %></td>
    <td><%= record.req_user_id %></td>
    <td><%= record.res_user_id  %></td>
    <td><%= record.battle_status  %></td>
  </tr>
  <% end %>
</table>
